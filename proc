-- Additional deletions based on the provided list of tables

-- ACCOUNTDOCUMENTS
DELETE ad
FROM ACCOUNTDOCUMENTS ad
INNER JOIN #AccountActivityIds aai ON ad.ACCOUNTACTIVITYID = aai.ACCOUNTACTIVITYID
PRINT convert(VARCHAR, getdate(), 120) + ' - deleted ACCOUNTDOCUMENTS: ' + convert(VARCHAR, @@rowcount)

-- TRANSACTIONS
DELETE t
FROM TRANSACTIONS t
INNER JOIN #AccountActivityIds aai ON t.ACCOUNTACTIVITYID = aai.ACCOUNTACTIVITYID
